<% if current_user %>
$('.flash.notice').html('<%= flash[:notice] %>');
$('.flash.notice').removeClass('hidden');
$('#login_button').val('Success!');
setTimeout(function(){ $('#container').addClass('hidden'); }, 500);
setTimeout(function(){
  $('#container').html('<%= j render partial: 'welcome/index' %>');
  $('#container').removeClass('hidden');
}, 1000);
console.log('what');
<% else %>
$('.flash.alert').html('<%= flash[:alert] %>');
$('.flash.alert').removeClass('hidden');
setTimeout(function(){
  $('#login_button').val('Sign up');
  $('.center_form_field input').removeClass('disabled');
}, 300);
<% end %>
