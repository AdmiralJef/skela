cloak('#user_nav');
setTimeout(function(){
    hide_title();
    $('#user_nav').html('<%= j render 'layouts/user_nav' %>');
    window.setTimeout(function(){
        decloak('#user_nav');
    }, 10);
}, 500);

setTimeout(function(){
  $('.content').html('<%= j render partial: 'login/new_session' %>');
  decloak('.content');
  $('#username').focus();
  flashNotice("Logged out successfully!");
  window.history.replaceState( {} , '', '<%= login_path %>' );
}, 600);
